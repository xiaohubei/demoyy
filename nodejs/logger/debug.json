{
    "appenders": [
        { "type": "console" },
        {
            "type": "dateFile",
            "filename": "logs/upmall_debug.log",
            "pattern": "-yyyy-MM-dd-hh",
            "category": "upmall"
        },
        {
            "type": "logLevelFilter",
            "level": "ERROR",
            "appender": {
                "type": "dateFile",
                "filename": "logs/errors_debug.log",
                "pattern": "-yyyy-MM-dd"
            }
        }
    ]
}